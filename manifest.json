{
  "manifest_version": 3,
  "name": "EXGecko",
  "version": "1.0.3",
  "description": "このプラグインは、eライブラリーでチートを使用します。",
  "content_scripts": [{
    "matches": ["https://ela.education.ne.jp/students/*"],
    "run_at": "document_end",
    "all_frames": true,
    "js": ["content-script.js"]
  }],
  "permissions": ["scripting", "activeTab"]
}